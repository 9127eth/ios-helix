// Template file showing required configuration
// Copy this file to Config.xcconfig and fill in your values

// Google Configuration
GOOGLE_CLIENT_ID = your.apps.googleusercontent.com
GOOGLE_REVERSED_CLIENT_ID = com.googleusercontent.apps.your-id
GOOGLE_API_KEY = your-google-api-key
GOOGLE_GCM_SENDER_ID = your-sender-id
GOOGLE_APP_ID = your-app-id
FIREBASE_DATABASE_URL = your-firebase-database-url

// Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY = your-firebase-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN = your-project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID = your-project-id
